   
   <section class="single-book">
        <% homeArray.forEach(book => { %>
            <img src=<%= book.image_url %> 
            alt= <%= book.title %> 
            />
            <h2><%= book.title %></h2>
            <h3><%= book.authors %></h3>
            <p><%= book.description %></p>
            <p><%= book.isbn %></p>
            <p><%= book.bookshelf %></p>
        <% }) %>
    </section>
